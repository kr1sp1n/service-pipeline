{
  "name": "service-pipeline",
  "version": "0.0.1",
  "description": "Pipelines as a service",
  "main": "./src/server.js",
  "scripts": {
    "test": "$(npm bin)/mocha --watch",
    "start": "node ./src/server.js",
    "dev": "$(npm bin)/node-dev ./src/server.js",
    "cov": "$(npm bin)/mocha -R mocha-spec-cov-alt",
    "html-cov": "$(npm bin)/mocha -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kr1sp1n/service-pipeline.git"
  },
  "keywords": [
    "service",
    "pipes",
    "pipelines"
  ],
  "author": "kr1sp1n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kr1sp1n/service-pipeline/issues"
  },
  "homepage": "https://github.com/kr1sp1n/service-pipeline",
  "dependencies": {
    "async": "^1.5.0",
    "body-parser": "^1.14.1",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "handlebars": "^4.0.3",
    "lodash": "^3.10.1",
    "mustache": "^2.2.0",
    "request": "^2.65.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "blanket": "^1.1.9",
    "coffee-script": "^1.10.0",
    "mocha": "^2.3.3",
    "node-dev": "^2.7.1",
    "should": "^7.1.1",
    "sinon": "^1.17.2",
    "mocha-spec-cov-alt": "git://github.com/kr1sp1n/mocha-spec-cov-alt.git#master"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "spec-cov": {
        "threshold": 100
      }
    }
  }
}
